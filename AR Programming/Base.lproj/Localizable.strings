// Basic card info
"function0.name" = "Start";
"function0.summary" = "Dit program starter her.";
"function0.description" = "Brug dette kort til at bestemme hvor programmet starter. Alle programmer skal indeholde et startkort. Startkortet skal være det første kort i rækken.";

"move.name" = "Gå";
"move.summary" = "Få robotten til at gå frem.";
"move.description" = "Dette kort vil få robotten til at gå ét skridt frem. Hvis robotten skal gå længere frem, så brug kortet flere gange. Du kan også bruge parameterkort til at ændre det antal skridt, robotten tager.\n\nRobotten går altid i den retning, den kigger i. Du kan bruge rotationskortene til at ændre robottens retning.";
"move.parameter" = "Tallet kan ændre hvor mange skridt robotten tager.";

"left.name" = "Venstre";
"left.summary" = "Drej robotten til venstre.";
"left.description" = "Brug dette kort til at dreje robotten én gang mod venstre - det er mod urets retning. Brug rotationskortene til at ændre den retning, som robotten går i.";
"left.parameter" = "Tallet kan ændre hvor langt robotten drejer.";

"right.name" = "Højre";
"right.summary" = "Drej robotten til højre.";
"right.description" = "Brug dette kort til at dreje robotten én gang mod højre - det er med urets retning. Brug rotationskortene til at ændre den retning, som robotten går i.";
"right.parameter" = "Tallet kan ændre hvor langt robotten drejer.";

"jump.name" = "Hop";
"jump.summary" = "Få robotten til at hoppe.";
"jump.description" = "Skal du nå noget der hænger oppe i luften? Når robotten ser dette kort, vil den lave et lille hop.";
"jump.parameter" = "Tallet kan ændre hvor højt robotten hopper.";

"pickup.name" = "Saml Op";
"pickup.summary" = "Få robotten til at samle noget op.";
"pickup.description" = "Brug dette kort til at få robotten til at samle noget op. Robotten kan kun samle ting op, hvis den står oven på dem.";
"pickup.parameter" = "";

"drop.name" = "Smid";
"drop.summary" = "Få robotten til at smide det den holder i hænderne.";
"drop.description" = "Brug dette kort til at få robotten til at smide det den har i hænderne. Robotten smider tingene der hvor den står.";
"drop.parameter" = "";

"loop.name" = "Gentag";
"loop.summary" = "Får en del af programmet til at gentage sig.";
"loop.description" = "Brug dette kort til at få en sekvens af kort til at gentage sig. Dette kort skal bruges sammen med et talkort og et grænsekort.";
"loop.parameter" = "Tallet kan ændre hvor mange gange gentagelseskortet skal gentage sig.";

"block.name" = "Grænse";
"block.summary" = "Dit gentagelse stopper her.";
"block.description" = "Brug dette kort til at vise slutningen på gentagelsen.";

"param1.name" = "Tal 1";
