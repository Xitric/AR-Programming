// Basic card info
"function0a.name" = "Start";
"function0a.summary" = "Dit program starter her.";
"function0a.description" = "Brug dette kort til at bestemme hvor programmet starter. Alle programmer skal indeholde et 'start' kort. 'Start' kortet skal være det første kort i rækken.";
"function0a.longDescription" = "Brug dette kort til at bestemme hvor programmet starter - ligesom du starter med at læse fra side 1 i en bog. Alle programmer skal indeholde et 'start' kort. 'Start' kortet skal være det første kort i rækken.\n\nHvis dit program ikke indeholder et 'start' kort, så virker det ikke.";

"move.name" = "Gå";
"move.summary" = "Få robotten til at gå frem.";
"move.description" = "Dette kort vil få robotten til at gå ét skridt frem. Robotten går altid i den retning, den kigger i. Du kan bruge 'venstre' og 'højre' kortene til at ændre robottens retning.";
"move.longDescription" = "Dette kort vil få robotten til at gå ét skridt frem. Robotten går altid i den retning, den kigger i. Du kan bruge 'venstre' og 'højre' kortene til at ændre robottens retning.\n\nHvis robotten skal gå længere frem, så brug kortet flere gange. Du kan også bruge talkort til at ændre det antal skridt, robotten tager.";
"move.parameter" = "Tallet kan ændre hvor mange skridt robotten tager.";

"left.name" = "Venstre";
"left.summary" = "Drej robotten til venstre.";
"left.description" = "Brug dette kort til at dreje robotten én gang mod venstre - det er mod urets retning. Dette kort kan bruges til at ændre den retning, som robotten går i.";
"left.longDescription" = "Brug dette kort til at dreje robotten én gang mod venstre - det er mod urets retning. Dette kort kan bruges til at ændre den retning, som robotten går i.\n\nHvis robotten skal dreje længere, kan du bruge kortet flere gange. Du kan også bruge talkort til at ændre hvor langt robotten drejer. Det kan du se i eksemplerne nedenunder.";
"left.parameter" = "Tallet kan ændre hvor langt robotten drejer.";

"right.name" = "Højre";
"right.summary" = "Drej robotten til højre.";
"right.description" = "Brug dette kort til at dreje robotten én gang mod højre - det er med urets retning. Dette kort kan bruges til at ændre den retning, som robotten går i.";
"right.longDescription" = "Brug dette kort til at dreje robotten én gang mod højre - det er med urets retning. Dette kort kan bruges til at ændre den retning, som robotten går i.\n\nHvis robotten skal dreje længere, kan du bruge kortet flere gange. Du kan også bruge talkort til at ændre hvor langt robotten drejer. Det kan du se i eksemplerne nedenunder.";
"right.parameter" = "Tallet kan ændre hvor langt robotten drejer.";

"jump.name" = "Hop";
"jump.summary" = "Få robotten til at hoppe.";
"jump.description" = "Dette kort får robotten til at lave et lille hop, der hvor den står. Du kan bruge kortet, hvis robotten skal nå noget, der hænger højt oppe i luften.";
"jump.longDescription" = "Dette kort får robotten til at lave et lille hop, der hvor den står. Du kan bruge kortet, hvis robotten skal nå noget, der hænger højt oppe i luften.\n\nDu kan bruge talkortene til at ændre hvor højt robotten hopper. Hvis du bruger kortet flere gange, så laver robotten bare flere små hop.";
"jump.parameter" = "Tallet kan ændre hvor højt robotten hopper.";

"pickup.name" = "Saml Op";
"pickup.summary" = "Få robotten til at samle noget op.";
"pickup.description" = "Brug dette kort til at få robotten til at samle noget op. Robotten kan kun samle ting op, hvis den står oven på dem. Desuden kan robotten kun holde én ting i hænderne ad gangen.";
"pickup.longDescription" = "Brug dette kort til at få robotten til at samle noget op. Robotten kan kun samle ting op, hvis den står oven på dem. Desuden kan robotten kun holde én ting i hænderne ad gangen.\n\nDette kort kan bruges hvis robotten skal flytte nogle ting rundt. 'Smid' kortet får robotten til at smide den ting, den har samlet op.";

"drop.name" = "Smid";
"drop.summary" = "Få robotten til at smide det den holder i hænderne.";
"drop.description" = "Brug dette kort til at få robotten til at smide det den har i hænderne. Robotten smider tingen, der hvor den står. Hvis robotten ikke har noget i hænderne, så sker der ikke noget.";
"drop.longDescription" = "Brug dette kort til at få robotten til at smide det den har i hænderne. Robotten smider tingen, der hvor den står. Hvis robotten ikke har noget i hænderne, så sker der ikke noget.\n\nDette kort kan bruges hvis robotten skal flytte nogle ting rundt. 'Saml op' kortet får robotten til at samle ting op, som den kan smide andre steder.";

"loop.name" = "Gentag";
"loop.summary" = "Får en del af programmet til at gentage sig.";
"loop.description" = "Brug dette kort, hvis du vil have en del af programmet til at køre flere gange. Kortet skal altid bruges sammen med et talkort og et 'grænse' kort.";
"loop.longDescription" = "Brug dette kort, hvis du vil have en del af programmet til at køre flere gange. Kortet skal altid bruges sammen med et talkort og et 'grænse' kort. Ellers virker programmet ikke.\n\nAlle kortene mellem 'gentag' og 'grænse' vil blive kørt én eller flere gange. Kort der ligger uden for 'gentag' og 'grænse' påvirkes altså ikke. Talkortet bestemmer, hvor mange gange kortene skal gentages. Det kan du se på eksemplerne nedenunder.\n\nHvis du vil lave meget komplicerede programmer, kan du prøve at lave gentagelser inde i andre gentagelser.";
"loop.parameter" = "Tallet kan ændre hvor mange gange en del af programmet skal gentages.";

"block.name" = "Grænse";
"block.summary" = "Fortæl robotten, hvor en gentagelse skal stoppe.";
"block.description" = "Dette kort skal bruges sammen med et 'gentag' kort. 'Gentag' kortet kan bruges til at køre en del af programmet flere gange. 'Grænse' kortet skal bruges til at fortælle, hvor en gentagelse stopper, så robotten ikke altid gentager hele programmet.";
"block.longDescription" = "Dette kort skal bruges sammen med et 'gentag' kort. 'Gentag' kortet kan bruges til at køre en del af programmet flere gange. 'Grænse' kortet skal bruges til at fortælle, hvor en gentagelse stopper, så robotten ikke altid gentager hele programmet.\n\nEt 'gentag' kort kan ikke bruges uden et 'grænse' kort, og et 'grænse' kort kan heller ikke bruges uden et 'gentag' kort.";

"param1.name" = "Tal 1";
"param1.summary" = "Talkort til dit program.";
"param1.description" = "Brug dette kort til at fortælle, at noget skal gøres 1 gang. Talkort kan bruges sammen med 'gentag' kortet og nogle andre som f.eks. 'gå'.";
"param1.longDescription" = "Brug dette kort til at fortælle, at noget skal gøres 1 gang.\n\nDu kan bruge talkortet sammen med et 'gentag' kort for at gentage en del af programmet 1 gang. Det er lidt kedeligt.\n\nDu kan også bruge talkortet sammen med f.eks. 'gå' for at få robotten til at gå 1 skridt frem. Det er også lidt kedeligt.\n\nDe andre talkort er mere interessante.";

"param2.name" = "Tal 2";
